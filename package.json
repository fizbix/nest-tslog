{
  "name": "@fizbix/nest-tslog",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A NestJs module wrapper for tslog",
  "keywords": [
    "nest",
    "nestjs",
    "tslog",
    "logging",
    "logger"
  ],
  "author": "Jakub Andrzejewski (Veskel01)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public"
  },
  "devDependencies": {
    "@nestjs/common": "9.3.9",
    "@nestjs/core": "9.3.9",
    "@nestjs/testing": "9.3.9",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.4",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.1.3",
    "rxjs": "7.8.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tslog": "^4.8.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0",
    "tslog": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "homepage": "https://github.com/fizbix/nest-tslog#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fizbix/nest-tslog.git"
  },
  "bugs": {
    "url": "https://github.com/fizbix/nest-tslog/issues"
  }
}
